msgid ""
"data/events_spider.xml$//event[@name=\"ENCOUNTER_CLANSMEN_INFESTATION\"]/choice/text"
msgstr "Continue..."

#: src-en/data/events_spider.xml:651
msgid ""
"data/events_spider.xml$//textList[@name=\"TEXT_ENCOUNTER_CLANSMEN_INFESTATION\"]/text[1]"
msgstr ""
"\"I grow tired of thessse ssspiderss. I came for a challenge, but thessse "
"arachnidsss are too easssy to kill. I long for a challenge... ssssuch asss "
"you!\" A Slug clansman attacks!"


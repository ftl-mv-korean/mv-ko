#: src-en/data/events_nebula.xml.append:1131
msgid ""
"data/events_nebula.xml.append$//eventList[@name=\"NEBULA_AUTO_DEFENSE_ITEM_CLOAK\"]/event[1]/text"
msgstr ""
"Du schaffst es am Schiff vorbeizuschleichen und die Station ohne Probleme zu"
" betreten."

#: src-en/data/events_nebula.xml.append:1133
msgid ""
"data/events_nebula.xml.append$//eventList[@name=\"NEBULA_AUTO_DEFENSE_ITEM_CLOAK\"]/event[1]/choice/text"

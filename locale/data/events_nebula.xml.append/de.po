msgstr "(Slug Crew) Lasse deinen Slug das Signal entziffern."

#: src-en/data/events_nebula.xml.append:251
msgid ""
"data/events_nebula.xml.append$//event[@name=\"NEBULA_SIGNAL\"]/choice[@req=\"LIST_CREW_SLUG\"]/event/text"
msgstr ""

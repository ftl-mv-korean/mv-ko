msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_ICECREAM\"]/choice[2]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_separatist.xml:186
msgid "data/events_separatist.xml$//event[@name=\"SEPERATIST_TRAP_SCAN\"]/text"
msgstr ""
"A neighboring Separatist ship hails you soon after you jump in. \"You are in"
" [luck] captain of Federation vessel. I am in possession of an expansive "
"data suite, including a map of this sector. Would you like to download?\""

#: src-en/data/events_separatist.xml:188
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_TRAP_SCAN\"]/choice[1]/text"
msgstr "Download the data."

#: src-en/data/events_separatist.xml:192
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_TRAP_SCAN\"]/choice[2]/text"
msgstr "No thanks."

#: src-en/data/events_separatist.xml:194
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_TRAP_SCAN\"]/choice[2]/event/text"
msgstr ""
"\"I see you have no interest in taking avantage of great [opportunities]\"."

#: src-en/data/events_separatist.xml:196
msgid ""
"data/events_separatist.xml$//event[@name=\"SEPERATIST_TRAP_SCAN\"]/choice[2]/event/choice[1]/text"
msgstr "Do something onboard the ship."


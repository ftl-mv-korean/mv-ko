msgstr "Заключить \"сделку всей жизни\" вместе с предзажигателем."

#: src-en/data/events_civilian.xml:1361
msgid ""
"data/events_civilian.xml$//event[@name=\"QUEST_GATLING_AUGMENT\"]/choice/event/choice[3]/text"

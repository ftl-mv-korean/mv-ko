msgid ""
"data/events_coalition.xml$//event[@name=\"NEBULA_COALITION_ABDUCTOR\"]/choice[@req=\"weapons\"]/event/text"
msgstr ""
"\"Ah yes yes, you are a skilled negotiator. Silly of me to think such tricks"
" would work on you. Heheh...\""


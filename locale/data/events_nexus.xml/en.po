msgid ""
"data/events_nexus.xml$//event[@name=\"SYLVAN_MAID\"]/choice[4]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_nexus.xml:2740
msgid "data/events_nexus.xml$//event[@name=\"SYLVAN_MAID_DEFEAT\"]/text"
msgstr ""
"The strange pink vessel has fallen, a shower of explicit items come tumbling"

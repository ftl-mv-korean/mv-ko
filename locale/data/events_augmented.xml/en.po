"data/events_augmented.xml$//event[@name=\"AUGMENTED_INQUISITION\"]/choice[5]/text"
msgstr "Ignore them."

#: src-en/data/events_augmented.xml:898
msgid ""
"data/events_augmented.xml$//event[@name=\"AUGMENTED_INQUISITION\"]/choice[5]/event/text"

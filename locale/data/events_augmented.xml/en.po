msgid ""
"data/events_augmented.xml$//event[@name=\"AUGMENTED_SEPARATISTS\"]/choice[1]/event/choice[1]/event/choice[2]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_augmented.xml:816
msgid ""
"data/events_augmented.xml$//event[@name=\"AUGMENTED_SEPARATISTS\"]/choice[1]/event/choice[@req=\"LIST_VARIANT_SEPARATIST\"]/text"
msgstr ""
"(Separatist Crew) \"Your mission is of [high] importance. However, so is "
"ours. Requesting: supplies.\""

#: src-en/data/events_augmented.xml:818
msgid ""
"data/events_augmented.xml$//event[@name=\"AUGMENTED_SEPARATISTS\"]/choice[1]/event/choice[@req=\"LIST_VARIANT_SEPARATIST\"]/event/text"
msgstr ""
"Your Separatist explains your mission to the other Engi. \"This request is "
"acceptable. Transferring supplies now.\""

#: src-en/data/events_augmented.xml:823
msgid ""
"data/events_augmented.xml$//event[@name=\"AUGMENTED_SEPARATISTS\"]/choice[1]/event/choice[@req=\"LIST_CREW_HARMONY\"]/text"
msgstr ""
"(Engi Crew) \"This is unacceptable. Preventing Separatist success is highest"
" priority.\""

#: src-en/data/events_augmented.xml:825
msgid ""
"data/events_augmented.xml$//event[@name=\"AUGMENTED_SEPARATISTS\"]/choice[1]/event/choice[@req=\"LIST_CREW_HARMONY\"]/event/text"

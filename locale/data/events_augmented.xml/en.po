msgid "data/events_augmented.xml$//event[@name=\"ANOINTED_SURRENDER\"]/text"
msgstr ""
"\"Ok, you winning. You may keep creature, not worth dying over. Taking this,"
" and leave us here. Apologies for attack.\""

#: src-en/data/events_augmented.xml:194
msgid ""
"data/events_augmented.xml$//event[@name=\"ANOINTED_SURRENDER\"]/choice[1]/text"

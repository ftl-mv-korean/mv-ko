msgid ""
"data/events_guards.xml$//textList[@name=\"TEXT_GUARD_ZOLTAN_CONTACT\"]/text[1]"
msgstr ""
"\"Nous prenons compte de votre appel. Qu'est-ce que vous désirez accomplir "
"en nous contactant?\""


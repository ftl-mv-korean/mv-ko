msgid ""
"data/events_guards.xml$//event[@name=\"GUARD_ROCKHOME\"]/choice[@req=\"rock_paladin\"]/text"
msgstr "(Membre d'équipage Paladin) Exiger une entrée immédiate."

#: src-en/data/events_guards.xml:9500
msgid ""
"data/events_guards.xml$//event[@name=\"GUARD_ROCKHOME\"]/choice[@req=\"rock_paladin\"]/event/text"
msgstr ""
"Les Rocailleux se réduisent immédiatement au silence suite à l'intervention "

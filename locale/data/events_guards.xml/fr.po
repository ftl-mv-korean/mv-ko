msgid ""
"data/events_guards.xml$//event[@name=\"GUARD_ZOLTAN_INTERACT\"]/choice[8]/text"
msgstr "Changer d'avis et laisser le garde tranquille."

#: src-en/data/events_guards.xml:1252
msgid ""
"data/events_guards.xml$//event[@name=\"GUARD_ZOLTAN_INTERACT\"]/choice[8]/event/text"
msgstr ""

#: src-en/data/events_guards.xml:5194
msgid ""
"data/events_guards.xml$//event[@name=\"NEBULA_REINFORCEMENTS_DYNASTY\"]/beaconType/unvisitedTooltip"
msgstr "Непосещенное место."

#: src-en/data/events_guards.xml:5195

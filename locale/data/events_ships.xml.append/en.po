#: src-en/data/events_ships.xml.append:1467
msgid ""
"data/events_ships.xml.append$//ship[@name=\"SHIP_FED_EGGMAN\"]/deadCrew/choice/event/crewMember"
msgstr "Robotnik"

#: src-en/data/events_ships.xml.append:1499
msgid ""
"data/events_ships.xml.append$//ship[@name=\"SHIP_REBEL_ESTATE_BAR\"]/destroyed/text"
msgstr "The Rebel vessel has been defeated. You scrap the remains."

#: src-en/data/events_ships.xml.append:1502

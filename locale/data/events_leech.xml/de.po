"data/events_leech.xml$//ship[@name=\"SHIP_LEECH_DREADNAUGHT\"]/destroyed/choice/text"
msgstr "Fortfahren..."

#: src-en/data/events_leech.xml:1162
msgid ""
"data/events_leech.xml$//ship[@name=\"SHIP_LEECH_DREADNAUGHT\"]/deadCrew/text"
msgstr ""

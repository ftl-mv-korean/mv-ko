#: src-en/data/events_orchid.xml:1591
msgid ""
"data/events_orchid.xml$//event[@name=\"GARDEN_SACRIFICE\"]/choice[1]/event/choice[6]/event/choice[2]/text"
msgstr "Ne rien faire."

#: src-en/data/events_orchid.xml:1599
msgid "data/events_orchid.xml$//event[@name=\"GARDEN_SACRIFICE\"]/choice[2]/text"
msgstr "Éviter les vaisseaux."

#: src-en/data/events_orchid.xml:1601
msgid ""
"data/events_orchid.xml$//event[@name=\"GARDEN_SACRIFICE\"]/choice[2]/event/text"
msgstr ""
"Si les Soleils Perdus sont en train d'effectuer un genre de rituel "
"sacrificiel, vous ne voulez pas y prendre part."

#: src-en/data/events_orchid.xml:1603

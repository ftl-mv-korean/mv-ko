msgid ""
"data/events_zoltan.xml.append$//eventList[@name=\"GREAT_EYE_LIST\"]/event[8]/choice[2]/event/text"
msgstr ""
"Les nanites se condensent en un Engi Séparatiste, entraîné à l'ingénierie "
"des boucliers."

#: src-en/data/events_zoltan.xml.append:3876

msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_ZOLTAN_REPAIRS\"]/choice[2]/event/text"
msgstr ""
"\"Federation identification provides services free of cost. For premium "
"service however: further payment will be required.\""

#: src-en/data/events_standard.xml:2704
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_ZOLTAN_REPAIRS\"]/choice[2]/event/choice[1]/text"

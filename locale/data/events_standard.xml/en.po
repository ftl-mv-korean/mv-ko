msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_ZOLTAN_GUARDS\"]/choice[1]/event/choice[2]/text"
msgstr "Fight back!"

#: src-en/data/events_standard.xml:2955
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_ZOLTAN_GUARDS\"]/choice[1]/event/choice[2]/event/text"
msgstr ""
"Your crew push off the guards and draw your weapons. Looks like a fight is "
"nigh!"

#: src-en/data/events_standard.xml:2959

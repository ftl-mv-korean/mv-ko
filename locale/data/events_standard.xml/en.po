msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_ZOLTAN_GUARDS\"]/choice[1]/event/text"
msgstr ""
"As soon as you dock, a swarm of Zoltan military guards grab your crew and "
"pin them to the ground. One of the guards pulls out a scanner and starts to "
"analyze them."

#: src-en/data/events_standard.xml:2949
msgid ""
"data/events_standard.xml$//event[@name=\"STOREEVENT_ZOLTAN_GUARDS\"]/choice[1]/event/choice[1]/text"
msgstr "What are you doing!?"


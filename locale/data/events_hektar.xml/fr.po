"data/events_hektar.xml$//event[@name=\"NEBULA_HEKTAR_REFUEL\"]/choice[1]/event/choice/event/choice[@req=\"LIST_VARIANT_BILLY\"]/text"
msgstr "(Billy) \"Il n’y a plus de dette, ssstupides crétins. \""

#: src-en/data/events_hektar.xml:1331
msgid ""
"data/events_hektar.xml$//event[@name=\"NEBULA_HEKTAR_REFUEL\"]/choice[1]/event/choice/event/choice[@req=\"LIST_VARIANT_BILLY\"]/event/text"

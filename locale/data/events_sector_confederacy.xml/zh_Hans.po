msgid ""
"data/events_sector_confederacy.xml$//event[@name=\"VAMPWEED_GARDEN\"]/choice[6]/text"
msgstr "我们没啥能帮上忙的。"

#: src-en/data/events_sector_confederacy.xml:179
msgid ""
"data/events_sector_confederacy.xml$//event[@name=\"VAMPWEED_GARDEN\"]/choice[6]/event/text"
msgstr "兰花族失望地点点头，准备毫无准备地继续前进——他只能浇点儿水，别的不会。"

#: src-en/data/events_sector_confederacy.xml:181

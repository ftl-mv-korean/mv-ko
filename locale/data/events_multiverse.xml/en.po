#: src-en/data/events_multiverse.xml:2676
msgid ""
"data/events_multiverse.xml$//textList[@name=\"TEXT_SYLVAN_GENERIC\"]/text[1]"
msgstr ""
"\"Sssstranger sssstranger, ssso niccce to sssee you again. Care for some "

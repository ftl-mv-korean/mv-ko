#: src-en/data/events_multiverse.xml:2163
msgid ""
"data/events_multiverse.xml$//event[@name=\"SYLVAN_PREIGNITER\"]/choice[1]/event/choice[2]/text"
msgstr "Yeah, I'd rather not fight you."

#: src-en/data/events_multiverse.xml:2165
msgid ""
"data/events_multiverse.xml$//event[@name=\"SYLVAN_PREIGNITER\"]/choice[1]/event/choice[2]/event/text"
msgstr ""
"\"Good choice, tiny man. Next time, tell Sylvan stay away from Tremor "
"stuff!\""

#: src-en/data/events_multiverse.xml:2167

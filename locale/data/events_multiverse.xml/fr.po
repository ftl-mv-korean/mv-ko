msgid ""
"data/events_multiverse.xml$//event[@name=\"SYLVAN_MARRIAGE_FAVOR\"]/choice/event/choice/event/choice/event/text"
msgstr ""
"\"Je vais vous donner les coordonnées de sssa cachette. Essssayez de "
"passsser la récupérer pour moi. Je vous en ssserais très reconnaissssant, "
"étranger!\""

#: src-en/data/events_multiverse.xml:3608
msgid ""
"data/events_multiverse.xml$//event[@name=\"SYLVAN_MARRIAGE_FAVOR\"]/choice/event/choice/event/choice/event/choice[1]/text"
msgstr "Voir ce qu'il propose."


#: src-en/data/events_rock.xml.append:1215
msgid ""
"data/events_rock.xml.append$//event[@name=\"ROCK_TEMPLE\"]/choice[2]/event/text"
msgstr "You'd have to be insane to raid a Rockman temple in Rock territory."

#: src-en/data/events_rock.xml.append:1217
msgid ""
"data/events_rock.xml.append$//event[@name=\"ROCK_TEMPLE\"]/choice[2]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_rock.xml.append:1221

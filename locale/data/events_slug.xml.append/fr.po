msgid ""
"data/events_slug.xml.append$//event[@name=\"NEBULA_CLAN_SUZERAIN\"]/choice[1]/event/choice[2]/event/choice[1]/event/choice[1]/event/choice[2]/event/text"
msgstr ""
"\"Après Guerre d'Expansion Limace, Coalition former et appliquer pacte, pas "

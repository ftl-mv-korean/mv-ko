#: src-en/data/events_slug.xml.append:439
msgid ""
"data/events_slug.xml.append$//eventList[@name=\"NEBULA_SLUG_BIOLAB_INVESTIGATE\"]/event[1]/choice[1]/event/text"
msgstr ""

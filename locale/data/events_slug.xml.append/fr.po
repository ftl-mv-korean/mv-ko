msgid ""
"data/events_slug.xml.append$//event[@name=\"NEBULA_CLAN_TAX\"]/choice[1]/event/choice/event/choice/event/choice[@req=\"LIST_CREW_SLUG\"]/text"
msgstr "(Membre d'équipage Limace) Essayer de raisonner avec le Chasseur."

#: src-en/data/events_slug.xml.append:5155
msgid ""
"data/events_slug.xml.append$//event[@name=\"NEBULA_CLAN_TAX\"]/choice[1]/event/choice/event/choice/event/choice[@req=\"LIST_CREW_SLUG\"]/event/text"
msgstr ""

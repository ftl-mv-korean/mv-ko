msgid ""
"data/events_slug.xml.append$//event[@name=\"NEBULA_CLAN_PHEROMONES\"]/choice[1]/event/choice/event/choice[2]/event/choice/event/choice/event/choice[1]/event/choice/text"
msgstr "Continue..."


msgid ""
"data/events_slug.xml.append$//event[@name=\"NEBULA_SLUG_DOORS\"]/choice[@req=\"ANTI_VIRUS\"]/text"
msgstr "(Anti-Virus) Counter the remote hacking."

#: src-en/data/events_slug.xml.append:895
msgid ""
"data/events_slug.xml.append$//event[@name=\"NEBULA_SLUG_DOORS\"]/choice[@req=\"ANTI_VIRUS\"]/event/text"
msgstr ""
"Your Anti-virus module automatically counters the digital assault and you "
"move in to fight the ship."

#: src-en/data/events_slug.xml.append:898

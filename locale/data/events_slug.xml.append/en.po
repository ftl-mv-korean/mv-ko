msgid ""
"data/events_slug.xml.append$//event[@name=\"SLUG_DISTRESS_TRICK\"]/choice[@req=\"LIST_CREW_MANTIS\"]/text"
msgstr "(Mantis Crew) Have your Mantis oversee the repairs."


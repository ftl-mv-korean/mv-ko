#: src-en/data/events_slug.xml.append:6214
msgid ""
"data/events_slug.xml.append$//event[@name=\"NEBULA_KNIGHT_SMUGGLER\"]/choice[2]/text"
msgstr "Ignore the Smugglers."

#: src-en/data/events_slug.xml.append:6216
msgid ""
"data/events_slug.xml.append$//event[@name=\"NEBULA_KNIGHT_SMUGGLER\"]/choice[2]/event/text"
msgstr "The smugglers slip away into the nebula not much longer."

#: src-en/data/events_slug.xml.append:6218
msgid ""
"data/events_slug.xml.append$//event[@name=\"NEBULA_KNIGHT_SMUGGLER\"]/choice[2]/event/choice[1]/text"
msgstr "Check the storage."

#: src-en/data/events_slug.xml.append:6222
msgid ""
"data/events_slug.xml.append$//event[@name=\"NEBULA_KNIGHT_SMUGGLER\"]/choice[2]/event/choice[2]/text"
msgstr "Do nothing."

#: src-en/data/events_slug.xml.append:6230
msgid ""
"data/events_slug.xml.append$//eventList[@name=\"KNIGHT_SMUGGLER_ATTACK\"]/event[1]/text"
msgstr "You arm the weapons and attack the smugglers!"

#: src-en/data/events_slug.xml.append:6233
msgid ""
"data/events_slug.xml.append$//eventList[@name=\"KNIGHT_SMUGGLER_ATTACK\"]/event[1]/choice/text"

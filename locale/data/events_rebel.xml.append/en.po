#: src-en/data/events_rebel.xml.append:3612
msgid ""
"data/events_rebel.xml.append$//event[@name=\"JERRY_AUTO_REPLICA\"]/choice[1]/event/choice[1]/event/choice[1]/text"
msgstr "Attack them!"

#: src-en/data/events_rebel.xml.append:3614
msgid ""
"data/events_rebel.xml.append$//event[@name=\"JERRY_AUTO_REPLICA\"]/choice[1]/event/choice[1]/event/choice[1]/event/text"
msgstr ""

msgid "data/events_ancient.xml$//event[@name=\"ANCIENT_LOST_DUDE\"]/text"
msgstr "To your surprise, a hibernating Obelisk is simply drifting in space."

#: src-en/data/events_ancient.xml:965
msgid ""
"data/events_ancient.xml$//event[@name=\"ANCIENT_LOST_DUDE\"]/choice[1]/text"

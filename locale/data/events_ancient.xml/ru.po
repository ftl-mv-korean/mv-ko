msgid ""
"data/events_ancient.xml$//event[@name=\"ANCIENT_SCULPTER_WIN\"]/choice[1]/text"
msgstr "Собрать астероиды."

#: src-en/data/events_ancient.xml:1581
msgid ""
"data/events_ancient.xml$//event[@name=\"ANCIENT_SCULPTER_WIN\"]/choice[1]/event/text"
msgstr ""

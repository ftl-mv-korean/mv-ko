msgid ""
"data/events_ancient.xml$//eventList[@name=\"ANCIENT_ENGI_HIDE_CHASE\"]/event[1]/text"
msgstr "Вы не можете отследить корабль энджи. Ты сдаешься и улетаешь."

#: src-en/data/events_ancient.xml:1324
msgid ""
"data/events_ancient.xml$//eventList[@name=\"ANCIENT_ENGI_HIDE_CHASE\"]/event[2]/text"

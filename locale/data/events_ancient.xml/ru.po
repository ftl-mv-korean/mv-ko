msgid ""
"data/events_ancient.xml$//event[@name=\"ROYAL_UPRISING\"]/choice[1]/event/choice/event/choice[2]/event/choice/event/text"
msgstr ""
"\"Боюсь, я не знаю, что это значит, но я решил, что мне все равно. Раз уж вы "

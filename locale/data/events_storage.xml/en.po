#: src-en/data/events_storage.xml:28971
msgid ""
"data/events_storage.xml$//event[@name=\"STORAGE_CHECK_CREW_SOLDIER_UPGRADE_ENGINES\"]/choice[@req=\"engines\""
" and @lvl=\"5\"]/event/text"
msgstr "Your crew is successfully able to upgrade the system."

#: src-en/data/events_storage.xml:28979
msgid ""
"data/events_storage.xml$//event[@name=\"STORAGE_CHECK_CREW_SOLDIER_UPGRADE_ENGINES\"]/choice[@req=\"engines\""
" and @lvl=\"6\"]/text"

"data/events_storage.xml$//event[@name=\"STORAGE_CHECK_CREW_MORPH_TRANSFORM\"]/choice[@req=\"MORPH_DATABASE\"]/event/choice[3]/text"
msgstr "Use the Rockman DNA. [Morph]"

#: src-en/data/events_storage.xml:33317
msgid ""
"data/events_storage.xml$//event[@name=\"STORAGE_CHECK_CREW_MORPH_TRANSFORM\"]/choice[@req=\"MORPH_DATABASE\"]/event/choice[3]/event/text"

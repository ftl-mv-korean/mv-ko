#: src-en/data/events_abandoned.xml:1056
msgid ""
"data/events_abandoned.xml$//event[@name=\"DISTRESS_ABANDONED_STOLEN_TECH\"]/choice[1]/event/choice[@req=\"ADV_SCANNERS\"]/event/choice/text"
msgstr "Contact the Engi."


#: src-en/data/events_abandoned.xml:1074
msgid ""
"data/events_abandoned.xml$//event[@name=\"DISTRESS_ABANDONED_STOLEN_TECH\"]/choice[2]/text"
msgstr "Run while you still can."

#: src-en/data/events_abandoned.xml:1076
msgid ""
"data/events_abandoned.xml$//event[@name=\"DISTRESS_ABANDONED_STOLEN_TECH\"]/choice[2]/event/text"
msgstr ""
"You power up the engines and fly far away until your computer can no longer "
"detect the signal."

#: src-en/data/events_abandoned.xml:1078
msgid ""
"data/events_abandoned.xml$//event[@name=\"DISTRESS_ABANDONED_STOLEN_TECH\"]/choice[2]/event/choice[1]/text"
msgstr "Do something onboard the ship."

#: src-en/data/events_abandoned.xml:1082
msgid ""
"data/events_abandoned.xml$//event[@name=\"DISTRESS_ABANDONED_STOLEN_TECH\"]/choice[2]/event/choice[2]/text"
